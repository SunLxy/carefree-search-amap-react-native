{
  "compilerOptions": {
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": "lib",
    "declaration": true,
    "jsx": "react-native",
    "lib": [
      "esnext"
    ],
    "resolveJsonModule": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": false,
    "target": "esnext",
    "module": "esnext",
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}